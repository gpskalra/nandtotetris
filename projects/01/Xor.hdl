/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a,out=NotA);
    Not(in=b,out=NotB);
    Nand(a=a,b=NotB,out=NandANotB);
    Nand(a=NotA,b=b,out=NandNotAB);
    Nand(a=NandANotB,b=NandNotAB,out=out);
}